$PICKE
Gosub ACTION From SUBPTH
GPE=1
If GOK>0
#Ne pas remettre en place la ligne du dessous qui est commentée
#En fait si on la décommente, on se rend compte lors du pickage qu'une commande est lockée
#mais dans ce cas, si la commande n'est pas lockée alors cela engendre un problème car le MDL de la première ligne n'est pas renseigné
#Si on commente la ligne, dans le cas d'une commande lockée, on n'a un mesage d'erreur sur un indice incorrect dans le tableau des ligne sur MDL
#  If([M:PTH1]NBLIG>1)Then
    [M:PTH1]MDL([M:PTH1]NBLIG-1)=[F:POH]MDL
    [M:PTH1]ZMDL([M:PTH1]NBLIG-1)=[F:POH]MDL
    [M:PTH1]ZBPAADD([M:PTH1]NBLIG-1)=[F:POH]BPAADD
    [M:PTH1]ZTYPPAL([M:PTH1]NBLIG-1)=[F:POQ]ZTYPPAL
    [M:PTH1]ZROTPLAT([M:PTH1]NBLIG-1)=[F:POH]ZROTPLAT
    [M:PTH1]ZROTBENN([M:PTH1]NBLIG-1)=[F:POH]ZROTBENN
    # If nolign<=5
        If [F:POH]BPAADD<>[M:PTH0]BPAADD
            [M:PTH0]BPAADD=[F:POH]BPAADD
            Affzo [M:PTH0]BPAADD
        Endif
    # Endif
#  Endif
    If [M:PTH0]ZCHFNUM=""
        [M:PTH0]ZCHFNUM=[F:POH]ZCHFNUM
        Affzo [M:PTH0]ZCHFNUM
        If [M:PTH0]ZTPLMAN=1
            Call VERIF_TPL([M:PTH0]PRHFCY,[M:PTH0]BPSNUM,[M:PTH0]BPAADD,[M:PTH0]ZCHFNUM)
        Endif
    Endif
    Call CALCUL_TPL([M:PTH0]ZTPLFLG,0,[M:PTH1]NBLIG-1)
Endif
If([F:POQ]ITMREF<>"" and GFLAG="RBE")Then
#    #On va récupérer l'enregistrement de l'article
#    Read [F:ITM]ITM0=[F:POQ]ITMREF
#    If(fstat=0)Then
#        If([F:ITM]TSICOD(4)="ZZZ")Then
        If([F:POQ]ITMREF="00LOTPALTRI")Then
            #On récupère le nombre de palette entrées sur la commande
            [M:PTH0]ZNBPALPLAT += [F:POH]ZNBPALPLAT
            Affzo [M:PTH0]ZNBPALPLAT
        Endif
#    Endif
Endif
Return

#################################################### ANNULE

$ANNULE
If [F:PTH]ZNHNUM<>""
  # Suppression du numéro de réception dans le bon de négoce
  Read [F:ZNH]ZNH0=[F:PTH]ZNHNUM
  If fstat=0
      If [F:ZNH]ZNHTPTHNUM=[F:PTH]PTHNUM
          [F:ZNH]ZNHTPTHNUM = ""
      Else
          [F:ZNH]ZNHPPTHNUM = ""
      Endif
      Rewrite [F:ZNH]
      If fstat
          Call ERREUR("Erreur de mise à jour du bon de négoce "+[F:SDH]ZNHNUM+". Transaction annulée.") From GESECRAN
          GOK=0
      Endif
  Elsif fstat=1
      Call ERREUR("Le bon de négoce "+[F:SDH]ZNHNUM+" à l'origine de cette réception est en cours d'utilisation sur un autre poste. Transaction annulée") From GESECRAN
      GOK=0
  Endif
Endif
If([M:PTH0]PTHNUM<>"")Then
  # Suppression du numéro de réception dans le bon de transport
  For [F:ZBTH]ZBTH0 Where [F:ZBTH]PTHNUM=[M:PTH0]PTHNUM
    [F:ZBTH]PTHNUM=""
    Rewrite [F:ZBTH]
    If fstat
        Call ERREUR("Erreur lors de la mise à jour du bon de transport "+[F:ZBTH]ZBTHNUM+". Transaction annulée.") From GESECRAN
        GOK=0
    Endif
  Next
Endif
Return

