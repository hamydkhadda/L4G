Subprog AM_ZTAXEGOAC(VALEUR)
Variable Integer VALEUR
## Taxe gasoil
If VALEUR=2
    Read [F:FCY]FCY0=[M:PTH0]PRHFCY
    If fstat>1
        Call ERREUR("Erreur de lecture du site. Taxe gasoil annulée.") From GESECRAN
        VALEUR = 1
        [M:PTH0]ZTAXEGOAC = 1
        Affzo [M:PTH0]ZTAXEGOAC
        End
    Elsif [F:FCY]ZTAXEGO=1
        Call ERREUR("Site non autorisé à la taxe gasoil. Taxe gasoil annulée.") From GESECRAN
        VALEUR = 1
        [M:PTH0]ZTAXEGOAC = 1
        Affzo [M:PTH0]ZTAXEGOAC
        End
    Endif
    Read [F:BPS]BPS0=[M:PTH0]BPSNUM
    If fstat>1
        Call ERREUR("Erreur de lecture du fournisseur. Taxe gasoil annulée.") From GESECRAN
        VALEUR = 1
        [M:PTH0]ZTAXEGOAC = 1
        Affzo [M:PTH0]ZTAXEGOAC
        End
    Elsif [F:BPS]ZTAXEGOAC=1
        Call ERREUR("Fournisseur non autorisé à la taxe gasoil. Taxe gasoil annulée.") From GESECRAN
        VALEUR = 1
        [M:PTH0]ZTAXEGOAC = 1
        Affzo [M:PTH0]ZTAXEGOAC
        End
    Endif
    If clalev([F:BPA])=0 : Local File BPADDRESS : Endif
    Read [F:BPA]BPA0=1;[M:PTH0]BPSNUM;[M:PTH0]BPAADD
    If fstat>1
        Call ERREUR("Erreur de lecture de l'adresse du fournisseur. Taxe gasoil annulée.") From GESECRAN
        VALEUR = 1
        [M:PTH0]ZTAXEGOAC = 1
        Affzo [M:PTH0]ZTAXEGOAC
        End
    Elsif [F:BPA]ZTAXEGOAC=1
        Call ERREUR("Adresse du fournisseur non autorisée à la taxe gasoil. Taxe gasoil annulée.") From GESECRAN
        VALEUR = 1
        [M:PTH0]ZTAXEGOAC = 1
        Affzo [M:PTH0]ZTAXEGOAC
        End
    Endif
Endif
End

